      <li>
        <type>PawnStart</type>
        <position>(0, 1)</position>
        <pawnIndex>1</pawnIndex>
      </li>
    </cellData>
  </AAM.AnimDef>

  <AAM.AnimDef ParentName="AnimExecutionBase">
